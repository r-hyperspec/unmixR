Package: unmixR
Type: Package
Title: Hyperspectral Unmixing Methods
Version: 0.2.490
Date: 2017-06-06
Authors@R: c(person("Anton", "Belov", role = "aut"),
	person("Conor", "McManus", role = "aut"),
	person("Claudia", "Beleites", role = c("aut", "cre"),
		email ="Claudia.Beleites@chemometrix.eu"),
